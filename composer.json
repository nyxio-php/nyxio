{
    "name": "nyxio/nyxio",
    "description": "nyxio framework",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "files": [
            "src/Helper/Directory/Directory.php",
            "src/Helper/Env/Env.php",
            "src/Helper/Reflection/Reflection.php",
            "src/Helper/Text/Text.php",
            "src/Helper/Url/Url.php"
        ],
        "psr-4": {
            "Nyxio\\": "src/",
            "Nyxio\\Config\\": "src/Config/",
            "Nyxio\\Container\\": "src/Container/",
            "Nyxio\\Contract\\": "src/Contract/",
            "Nyxio\\Event\\": "src/Event/",
            "Nyxio\\Helper\\": "src/Helper/",
            "Nyxio\\Http\\": "src/Http/",
            "Nyxio\\Kernel\\": "src/Kernel/",
            "Nyxio\\Provider\\": "src/Provider/",
            "Nyxio\\Routing\\": "src/Routing/",
            "Nyxio\\Validation\\": "src/Validation/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nyxio\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Aleksey Ishutin",
            "email": "ishutin.alexey@gmail.com"
        }
    ],
    "require": {
        "ext-mbstring": "*",
        "ext-openswoole": "*",
        "nyholm/psr7": "^1.4",
        "php": ">=8.1",
        "php-http/message-factory": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symplify/monorepo-builder": "^10.0",
        "vimeo/psalm": "^4.16"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": false
        }
    },
    "replace": {
        "nyxio/config": "1.0.2",
        "nyxio/container": "1.0.2",
        "nyxio/contract": "1.0.2",
        "nyxio/event": "1.0.2",
        "nyxio/helper": "1.0.2",
        "nyxio/http": "1.0.2",
        "nyxio/kernel": "1.0.2",
        "nyxio/provider": "1.0.2",
        "nyxio/routing": "1.0.2",
        "nyxio/validation": "1.0.2"
    },
    "minimum-stability": "dev"
}
